y";s:18:"/works/OL20134639W";s:4:"type";s:4:"work";s:4:"seed";a:8:{i:0;s:18:"/books/OL27314614M";i:1;s:18:"/works/OL20134639W";i:2;s:29:"/subjects/juvenile_literature";i:3;s:38:"/subjects/criticism_and_interpretation";i:4;s:27:"/subjects/handbooks_manuals";i:5;s:45:"/subjects/harry_potter_(fictitious_character)";i:6;s:23:"/subjects/stories_plots";i:7;s:19:"/authors/OL7629217A";}s:5:"title";s:29:"The Unauthorized Harry Potter";s:13:"title_suggest";s:29:"The Unauthorized Harry Potter";s:12:"has_fulltext";b:1;s:13:"edition_count";i:1;s:11:"edition_key";a:1:{i:0;s:11:"OL27314614M";}s:12:"publish_date";a:1:{i:0;s:12:"Sep 19, 2006";}s:12:"publish_year";a:1:{i:0;i:2006;}s:18:"first_publish_year";i:2006;s:3:"lcc";a:1:{i:0;s:29:"PR-6068.00000000.O93 Z57 2006";}s:4:"isbn";a:2:{i:0;s:10:"0979233100";i:1;s:13:"9780979233104";}s:15:"last_modified_i";i:1606758339;s:13:"ebook_count_i";i:1;s:2:"ia";a:1:{i:0;s:24:"unauthorizedharr0000cast";}s:13:"public_scan_b";b:0;s:15:"ia_collection_s";s:102:"fav-booksarenice23;inlibrary;drakeuniversity-ol;internetarchivebooks;printdisabled;fav-viniciushpmuniz";s:17:"lending_edition_s";s:11:"OL27314614M";s:20:"lending_identifier_s";s:24:"unauthorizedharr0000cast";s:15:"printdisabled_s";s:11:"OL27314614M";s:17:"cover_edition_key";s:11:"OL27314614M";s:7:"cover_i";i:8785423;s:9:"publisher";a:1:{i:0;s:7:"BORDERS";}s:10:"author_key";a:1:{i:0;s:10:"OL7629217A";}s:11:"author_name";a:1:{i:0;s:16:"ADAM-TROY CASTRO";}s:7:"subject";a:7:{i:0;s:19:"Juvenile literature";i:1;s:28:"Criticism and interpretation";i:2;s:18:"Handbooks, manuals";i:3;s:35:"Harry Potter (Fictitious character)";i:4;s:14:"Stories, plots";i:5;s:15:"Accessible book";i:6;s:15:"Protected DAISY";}s:9:"id_amazon";a:1:{i:0;s:10:"B000UMNJ10";}s:9:"ia_box_id";a:1:{i:0;s:9:"IA1940914";}s:15:"publisher_facet";a:1:{i:0;s:7:"BORDERS";}s:13:"subject_facet";a:7:{i:0;s:15:"Accessible book";i:1;s:28:"Criticism and interpretation";i:2;s:18:"Handbooks, manuals";i:3;s:35:"Harry Potter (Fictitious character)"